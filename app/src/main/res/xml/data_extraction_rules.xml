<?xml version="1.0" encoding="utf-8"?>
<!--
   Data extraction rules for Android 12+.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude API Keys from Cloud Backup -->
        <exclude domain="sharedpref" path="api_keys_secure.xml"/>
    </cloud-backup>
    
    <device-transfer>
        <!-- Exclude API Keys from Device-to-Device Transfer -->
        <exclude domain="sharedpref" path="api_keys_secure.xml"/>
    </device-transfer>
</data-extraction-rules>